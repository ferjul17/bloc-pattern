{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "ci": {
      "dependsOn": ["prettier:check", "eslint:check","test:coverage"],
      "outputMode": "new-only"
    },
    "prettier:check": {
      "outputMode": "new-only"
    },
    "eslint:check": {
      "outputMode": "new-only"
    },
    "test:coverage": {
      "outputMode": "new-only"
    }
  }
}